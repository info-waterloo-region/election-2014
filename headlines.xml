---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>{{ site.title }}</title>
 <link href="{{ site.url }}/headlines.xml" rel="self"/>
 <link href="{{ site.url }}/headlines/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}</id>
 <author>
   <name>{{ site.author.name }}</name>
   <email>{{ site.author.email }}</email>
 </author>
{% for h in site.data.headlines %}
 <entry>
   <title>{{h.Title}}</title>
   <link href="{{h.URL}}"/>
   <updated>{{h.Date | date_to_xmlschema }}</updated>
   <id>{{h.URL}}</id>
 </entry>
{% endfor %}
</feed>
